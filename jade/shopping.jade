!!!
html(lang="de")
  head
    title= title
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name="viewport", content="width=device-width,initial-scale=1,maximum-scale=1")
    link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Droid+Sans')
    link(rel='stylesheet/less', type='text/css', href='styles/client.less')
    script(type='text/javascript', src='js/libs/socketio/socket.io.js')
    script(type='text/javascript', src='js/libs/jquery/jquery.js')
    script(type='text/javascript', src='js/libs/less/less-1.3.3.min.js')
    script(type='text/javascript', src='js/libs/require/require.js', data-main='js/shopping')
  body
    div#log
    div#main
      section#header
      section#body
        div#app
      section#footer
    script#modelbase(type='text/template')
      div(class="model modeltype-<%= modelType%> viewsize-<%= viewSize%>")
        <%= name%>
    script#collectionbase(type='text/template')
      div(class="collection collectiontype-<%= collectionType%> viewsize-<%= viewSize%>")
        <%= modelViews %>
    script(type='text/javascript')
        function log(val) {
          $('#log').text(val);
        }
